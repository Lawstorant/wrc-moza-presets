<?xml version="1.0" encoding="utf-8"?>
<action_map name="moza_handbrake_hbp" sub_device_name="moza_handbrake" library="lib_direct_input" version="3">
  <axis_map>
    <axis name="di_slider_1" axis_set_name="device" axis_set_id="handbrake" />
    <axis name="di_button_120" axis_set_name="device" axis_set_id="handbrake" />
  </axis_map>
  <axis_defaults>
    <axis deadzone="0.01" saturation="1" name="di_slider_1">
      <action deadzone="0.2" saturation="0.8" name="driving.handbrake" />
    </axis>
  </axis_defaults>
  <group name="driving">
    <action name="handbrake">
      <axis name="di_slider_1" />
      <axis name="di_button_120" />
    </action>
  </group>
</action_map>
